    <page id="remi.html">
        <title lang="it" value="Remember the milk"/>
        <css value="custom/css/main.css"/>
        <meta lang="it" name="description" value="A Firefox OS app to help not forget things anymore."/>
        <meta lang="it" name="keywords" value="firefoxos,project,javascript,html,js,file,script,class,function,firefox,app,webapp"/>
